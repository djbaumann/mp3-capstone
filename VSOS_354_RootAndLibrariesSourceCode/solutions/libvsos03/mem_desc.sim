MEMORY {
page 0:
	# program RAM
	reset: origin = 0x0, length = 2
	modu_int: origin = 0x23, length = 1
	ram_prog: origin = 0x50, length = 0x0fd0
page 1:
	# data X-RAM
	data_x: origin = 0x0, length = 0x4000
page 2:
	# data Y-RAM
	data_y: origin = 0x0, length = 0x4000
	stdio: origin = 0x7000, length = 2, option = "vsstdio"
}

CORE {
	frequency = 12.288MHz
}
